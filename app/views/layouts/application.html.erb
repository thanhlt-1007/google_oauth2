<!DOCTYPE html>
<html>
  <head>
    <title>GoogleOauth2</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if notice %>
      <h1>Notice: <%= notice %></h1>
    <% end %>

    <% if alert %>
      <h1>Alert: <%= alert %></h1>
    <% end %>

    <%= yield %>
  </body>
</html>
